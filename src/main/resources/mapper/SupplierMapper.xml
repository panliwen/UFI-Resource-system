<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ufi.pdioms.resource.supplier.dao.SupplierDao">

    <select id="findSupplierCount" resultType="java.lang.Integer">
       SELECT COUNT(*) from tb_supplier where supplier=#{search}
    </select>

    <select id="findContactsCount" resultType="java.lang.Integer">
          SELECT COUNT(*) from tb_supplier where contacts=#{search}
    </select>


</mapper>