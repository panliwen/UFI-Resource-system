<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ufi.pdioms.resource.battery.dao.BatteryDao">

    <select id="findNumberCount" resultType="java.lang.Integer">
       SELECT COUNT(*) from tb_battery where number=#{search}
    </select>

    <select id="findSnCount" resultType="java.lang.Integer">
          SELECT COUNT(*) from tb_battery where sn=#{search}
    </select>


</mapper>